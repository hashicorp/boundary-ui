{
  "name": "rose",
  "version": "0.0.0",
  "description": "Styles and components for Boundary.",
  "license": "MPL-2.0",
  "author": "HashiCorp",
  "keywords": [
    "ember-addon"
  ],
  "scripts": {
    "build:development": "ember build",
    "build": "ember build --environment=production && cp metadata.json dist/metadata.json",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:sass": "stylelint 'app/styles/**/*.scss'",
    "lint:sass:fix": "stylelint 'app/styles/**/*.scss' --fix",
    "format": "npm-run-all format:*",
    "format:hbs": "prettier --write '{addon,tests}/**/*.hbs'",
    "format:js": "prettier --write '{addon,app,config,stories,tests}/**/*.js' *.js",
    "format:mdx": "prettier --write 'addon/**/*.mdx'",
    "format:sass": "prettier --write '{addon,app}/**/*.scss'",
    "start": "ember serve",
    "test": "ember exam --test-port 0 --split=4 --parallel=1 --random",
    "test:ember-compatibility": "ember try:each",
    "precommit": "yarn lint && yarn format && yarn test",
    "doc:toc": "doctoc README.md"
  },
  "dependencies": {
    "@embroider/test-setup": "^2.1.1",
    "@hashicorp/design-system-components": "^2.8.0",
    "codemirror": "5.65.7",
    "ember-auto-import": "^2.4.2",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-clipboard": "^0.16.0",
    "ember-cli-htmlbars": "^6.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-composable-helpers": "^5.0.0",
    "ember-focus-trap": "^1.0.1",
    "ember-named-blocks-polyfill": "^0.2.5",
    "ember-radio-button": "github:yapplabs/ember-radio-button.git#b2ab65f",
    "ember-uuid": "^2.1.0",
    "jsonlint": "^1.6.3",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-ember-modules-api-polyfill": "^2.12.0",
    "babel-plugin-htmlbars-inline-precompile": "3",
    "broccoli-asset-rev": "^3.0.0",
    "doctoc": "^2.2.0",
    "ember-cli": "^4.4.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-exam": "^8.0.0",
    "ember-export-application-global": "^2.0.1",
    "ember-inline-svg": "^1.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-modifier": "^3.2.7",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.8",
    "ember-template-lint": "^4.14.0",
    "ember-template-lint-plugin-prettier": "^4.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-qunit": "^7.3.0",
    "jest": "^27.4.7",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "react": "^16",
    "react-dom": "^16",
    "react-syntax-highlighter": "^15.5.0",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^6.0.0",
    "webpack": "^5.76.0"
  },
  "engines": {
    "node": "12.* || 14.* || 16.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.hbs": "ember-template-lint --fix",
    "*.scss": "stylelint --fix",
    "*.{js,scss,mdx,yaml}": "prettier --write"
  }
}
