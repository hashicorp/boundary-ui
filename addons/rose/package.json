{
  "name": "rose",
  "version": "0.0.0",
  "description": "Styles and components for Boundary.",
  "license": "MPL-2.0",
  "author": "HashiCorp",
  "keywords": [
    "ember-addon"
  ],
  "scripts": {
    "build:development": "ember build",
    "build": "ember build --environment=production && cp metadata.json dist/metadata.json",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:sass": "sass-lint -v -q",
    "format": "npm run format:js && npm run format:sass",
    "format:js": "prettier --write '{addon,app,config,stories,tests}/**/*.js' *.js",
    "format:sass": "prettier --write addon/**/*.scss",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prestorybook": "yarn build && yarn storybook:build",
    "storybook": "start-storybook -p 6006 -s dist",
    "storybook:build": "build-storybook -s dist && sb extract",
    "doc:toc": "doctoc README.md"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.8",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-composable-helpers": "^4.5.0",
    "ember-focus-trap": "^0.7.0",
    "ember-named-blocks-polyfill": "^0.2.4",
    "ember-radio-button": "^2.0.1",
    "ember-uuid": "^2.1.0",
    "emberx-select": "github:adopted-ember-addons/emberx-select#bc5f774"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@hashicorp/flight-icons": "^2.0.1",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-docs": "^6.3.8",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addons": "^6.3.9",
    "@storybook/api": "^6.3.12",
    "@storybook/cli": "^6.3.10",
    "@storybook/components": "^6.3.12",
    "@storybook/core-events": "^6.3.0",
    "@storybook/ember": "^6.3.8",
    "@storybook/ember-cli-storybook": "^0.4.0",
    "@storybook/theming": "^6.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "broccoli-asset-rev": "^3.0.0",
    "doctoc": "^2.0.1",
    "ember-auto-import": "^1.12.0",
    "ember-cli": "~4.1.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-inline-svg": "^1.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-modifier": "^1.0.3",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.4",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.7.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "qunit": "^2.17.1",
    "qunit-dom": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.41.0",
    "sass-lint": "^1.13.1",
    "webpack": "^5.59.1"
  },
  "engines": {
    "node": "12.* || 14.* || 16.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
