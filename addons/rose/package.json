{
  "name": "rose",
  "version": "0.0.0",
  "description": "Styles and components for Boundary.",
  "license": "MPL-2.0",
  "author": "HashiCorp",
  "keywords": [
    "ember-addon"
  ],
  "scripts": {
    "build:development": "ember build",
    "build": "ember build --environment=production",
    "lint": "yarn lint:hbs && yarn lint:js && yarn lint:sass",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:sass": "sass-lint -v -q",
    "format": "npm run format:js && npm run format:sass",
    "format:js": "prettier --write '{addon,app,config,stories,tests}/**/*.js' *.js",
    "format:sass": "prettier --write addon/**/*.scss",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "prestorybook": "yarn build && yarn storybook:build",
    "storybook": "start-storybook -p 6006 -s dist",
    "storybook:build": "build-storybook -s dist",
    "doc:toc": "doctoc README.md"
  },
  "dependencies": {
    "babel-loader": "^8.1.0",
    "ember-cli-babel": "^7.17.2",
    "ember-cli-htmlbars": "^4.2.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@hashicorp/structure-icons": "^1.8.1",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/ember": "^5.3.18",
    "@storybook/ember-cli-storybook": "^0.2.1",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "doctoc": "^1.4.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.16.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-inline-svg": "^1.0.0",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-modifier": "^1.0.3",
    "ember-qunit": "^4.6.0",
    "ember-radio-button": "^2.0.1",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.12.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "ember-uuid": "^2.1.0",
    "emberx-select": "github:adopted-ember-addons/emberx-select#bc5f774",
    "eslint-plugin-ember": "^7.7.2",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.7.0",
    "prettier": "^2.0.5",
    "qunit-dom": "^1.0.0",
    "sass": "^1.26.5",
    "sass-lint": "^1.13.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
