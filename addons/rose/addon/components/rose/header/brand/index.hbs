<LinkTo
  ...attributes
  class='rose-header-brand'
  title={{@text}}
  @route={{if @route @route 'scopes.scope'}}
  @model={{if @model @model 'global'}}
>

  {{#if @logo}}
    {{inline-svg @logo}}
  {{/if}}

  {{#if @text}}
    <h1 class='rose-header-brand-text'>{{@text}}</h1>
  {{/if}}

</LinkTo>