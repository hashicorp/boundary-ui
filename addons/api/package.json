{
  "name": "api",
  "version": "0.0.0",
  "description": "API data layer for Boundary.",
  "license": "MPL-2.0",
  "author": "HashiCorp",
  "keywords": [
    "ember-addon"
  ],
  "scripts": {
    "build:development": "ember build",
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "format": "yarn run format:js",
    "format:js": "prettier --write '{addon,app,config,stories,tests,mirage}/**/*.js' *.js",
    "start": "ember serve",
    "test": "ember test",
    "test:ember-compatibility": "ember try:each",
    "precommit": "yarn lint && yarn format && yarn test",
    "doc:toc": "doctoc README.md"
  },
  "dependencies": {
    "ember-can": "^3.1.0",
    "ember-cli-babel": "^7.26.8",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-mirage": "^2.2.0",
    "ember-data": "^3.20.0",
    "ember-data-model-fragments": "5.0.0-beta.2",
    "ember-fetch": "^8.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "doctoc": "^2.2.0",
    "ember-auto-import": "^1.12.0",
    "ember-cli": "~4.1.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-copy": "^2.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.4",
    "ember-template-lint": "^3.7.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-qunit": "^6.2.0",
    "js-bexpr": "hashicorp/js-bexpr#9b4a4b54d85eba67fdfc0990133d1518d890b1e1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "qunit": "^2.17.1",
    "qunit-dom": "^2.0.0"
  },
  "engines": {
    "node": "12.* || 14.* || 16.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.hbs": "ember-template-lint --fix",
    "*.{js,scss}": "prettier --write"
  }
}
