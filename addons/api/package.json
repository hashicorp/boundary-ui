{
  "name": "api",
  "version": "0.0.0",
  "description": "API data layer for Boundary.",
  "license": "MPL-2.0",
  "author": "HashiCorp",
  "keywords": [
    "ember-addon"
  ],
  "scripts": {
    "build:development": "ember build",
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "format": "yarn run format:js",
    "format:js": "prettier --write '{addon,app,config,stories,tests,mirage}/**/*.js' *.js",
    "start": "ember serve",
    "test": "ember test",
    "test:ember-compatibility": "ember try:each",
    "precommit": "yarn lint && yarn format && yarn test",
    "doc:toc": "doctoc README.md"
  },
  "dependencies": {
    "ember-can": "^4.2.0",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^6.1.0",
    "ember-cli-mirage": "^2.4.0",
    "ember-copy": "^2.0.1",
    "ember-data": "~4.4.0",
    "ember-fetch": "^8.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.6.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "doctoc": "^2.2.0",
    "ember-auto-import": "^2.4.3",
    "ember-cli": "^4.4.1",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.14.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-qunit": "^7.3.0",
    "js-bexpr": "hashicorp/js-bexpr#9b4a4b54d85eba67fdfc0990133d1518d890b1e1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.73.0"
  },
  "engines": {
    "node": "12.* || 14.* || 16.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.hbs": "ember-template-lint --fix",
    "*.{js,scss,yaml}": "prettier --write"
  }
}
