{
  "name": "boundary-desktop",
  "productName": "Boundary",
  "version": "0.0.0",
  "private": true,
  "description": "Electron UI for client for Boundary.",
  "license": "BUSL-1.1",
  "author": "HashiCorp",
  "main": "src/index.js",
  "scripts": {
    "build:cli": "node -e \"require('./config/cli.js').setup()\"",
    "build:config": "node -e \"require('./config/desktop.js').setup()\"",
    "build:all": "yarn build:cli && yarn build:config",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "yarn build:all && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "config": {
    "forge": "config/forge.config.js"
  },
  "dependencies": {
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.1.2",
    "electron-store": "^8.2.0",
    "node-html-parser": "^5.3.3",
    "node-pty": "^1.1.0-beta33",
    "semver": "^7.5.3",
    "shell-env": "^3.0.1",
    "shell-quote": "^1.7.3",
    "tree-kill": "^1.2.2",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-deb": "^7.7.0",
    "@electron-forge/maker-dmg": "^7.7.0",
    "@electron-forge/maker-squirrel": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "electron": "35.1.2",
    "unzipper": "^0.11.5"
  },
  "pnpm": {
    "overrides": {
      "highlight.js": "^10.4.1",
      "node-gyp": "^10.0.0"
    }
  }
}
