<Hds::AppSideNav::Portal
  @ariaLabel='Primary'
  class='side-nav-dark-mode-overrides'
  as |Nav|
>
  <Nav.Link
    @icon='crosshair'
    @text={{t 'resources.target.title_plural'}}
    @route='scopes.scope.projects.targets'
  />
  <Nav.Link
    @icon='entry-point'
    @text={{t 'resources.session.title_plural'}}
    @route='scopes.scope.projects.sessions'
  />
  <Nav.Link
    @icon='settings'
    @text={{t 'titles.settings'}}
    @route='scopes.scope.projects.settings'
  />
</Hds::AppSideNav::Portal>

{{outlet}}