{{title @model.displayName}}

<Rose::Layout::Page as |page|>

  <page.breadcrumbs>
    <BreadCrumbs />
  </page.breadcrumbs>

  <page.header>
    {{outlet "header"}}
  </page.header>

  <page.navigation>
    {{outlet "navigation"}}
  </page.navigation>

  <page.actions>
    {{outlet "actions"}}
  </page.actions>

  <page.body>
    {{outlet}}
  </page.body>

</Rose::Layout::Page>
