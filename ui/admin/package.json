{
  "name": "admin",
  "version": "0.0.0",
  "private": true,
  "description": "Admin UI for Boundary.",
  "license": "MPL-2.0",
  "author": "HashiCorp",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "doc:toc": "doctoc README.md",
    "build:development": "ember build",
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:sass": "sass-lint -v -q",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write '{app,config,stories,tests}/**/*.js' *.js",
    "format:sass": "prettier --write app/**/*.scss",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "clean:coverage": "rm -rf coverage_*",
    "pretest": "yarn clean:coverage",
    "test": "npm-run-all test:*",
    "test:ember": "COVERAGE=true ember exam --random",
    "posttest": "ember coverage-merge && yarn clean:coverage"
  },
  "dependencies": {
    "ember-named-blocks-polyfill": "^0.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^1.0.2",
    "@ember/test-helpers": "^2.1.4",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "clipboard": "^2.0.6",
    "core": "*",
    "doctoc": "^1.4.0",
    "ember-a11y-testing": "^3.0.0",
    "ember-auto-import": "^1.10.1",
    "ember-breadcrumbs": "^0.2.3",
    "ember-cli": "~3.24.0",
    "ember-cli-app-version": "^4.0.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-code-coverage": "^1.0.2",
    "ember-cli-content-security-policy": "^1.1.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^2.0.2",
    "ember-cli-terser": "^4.0.1",
    "ember-cli-update": "^0.54.6",
    "ember-concurrency": "^1.3.0",
    "ember-exam": "^6.0.1",
    "ember-export-application-global": "^2.0.1",
    "ember-feature-flags": "^6.0.0",
    "ember-inline-svg": "^1.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-modifier": "^2.1.1",
    "ember-notify": "^6.0.0",
    "ember-page-title": "^6.0.3",
    "ember-qunit": "^5.1.1",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.24.0",
    "ember-template-lint": "^2.15.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-ember": "^10.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.13.0",
    "qunit-dom": "^1.6.0",
    "rose": "*",
    "sass": "^1.29.0",
    "sass-lint": "^1.13.1",
    "sinon": "^9.2.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  }
}
