<Rose::Form
  class='full-width'
  @onSubmit={{@submit}}
  @cancel={{@cancel}}
  @disabled={{@model.isSaving}}
  as |form|
>
  <WorkerFilterGenerator @model={{@model}} @name={{@name}} />

  <form.actions
    @submitText={{t 'actions.save'}}
    @cancelText={{t 'actions.cancel'}}
  />
</Rose::Form>