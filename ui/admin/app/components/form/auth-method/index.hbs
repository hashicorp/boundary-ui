{{#if @model.type}}
  <this.authMethodFormComponent
    @model={{@model}}
    @edit={{@edit}}
    @submit={{@submit}}
    @cancel={{@cancel}}
    @removeItemByIndex={{@removeItemByIndex}}
    @addStringItem={{@addStringItem}}
    @addAccountMapItem={{@addAccountMapItem}}
    @addRow={{this.addRow}}
    @removeRow={{this.removeRow}}
    @updateRow={{this.updateRow}}
    @createEmptyValue={{this.createEmptyValue}}
    @createEmptyKeyValue={{this.createEmptyKeyValue}}
  />
{{/if}}