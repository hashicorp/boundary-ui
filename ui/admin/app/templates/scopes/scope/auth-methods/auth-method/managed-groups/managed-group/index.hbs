<Rose::Layout::Page as |page|>

  <page.breadcrumbs>
    <BreadCrumbs />
  </page.breadcrumbs>

  <page.header>
    <AuthMethods::AuthMethod::ManagedGroups::ManagedGroup::Header
      @model={{@model}}
    />
  </page.header>

  <page.navigation>
    <AuthMethods::AuthMethod::ManagedGroups::ManagedGroup::Nav
      @model={{@model}}
    />
  </page.navigation>

  <page.actions>
    <AuthMethods::AuthMethod::ManagedGroups::ManagedGroup::Actions
      @model={{@model}}
    />
  </page.actions>

  <page.body>
    <Form::ManagedGroup
      @model={{@model}}
      @submit={{route-action 'save' @model}}
      @cancel={{route-action 'cancel' @model}}
    />
  </page.body>

</Rose::Layout::Page>